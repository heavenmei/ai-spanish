.navbar-left {
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	column-gap: 16rpx;
	margin-left: 8px;
	font-size: 18px;
	font-weight: 600;
}

.custom-navbar__icon {
	background-color: #fff;
	border-radius: 50%;
	padding: 4px;
	cursor: pointer;
}

.container{
	display: flex;
	flex-direction: column;
	overflow: auto;
	padding: 0;
}
.new-btn{
	padding: 40rpx 30rpx 0;

}

.message-card-list{
	flex: 1;
	display: flex;
	flex-direction: column;
	gap:12px;
	overflow: auto;
	padding: 0 30rpx;
}