.navbar-left {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  column-gap: 16rpx;
  margin-left: 8px;
  font-size: 18px;
  font-weight: 600;
}

.custom-navbar__icon {
  background-color: #fff;
  border-radius: 50%;
  padding: 4px;
  cursor: pointer;
}

.scenarios-list-wrap {
  display: flex;
  flex-wrap: wrap;
  padding: 10px 0;
  background: #fff;
  gap: 12px;
  overflow: auto;
}
